<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/linearLayout"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginTop="@dimen/_15sdp"
    android:background="@color/white"
    android:orientation="vertical">


    <ImageView
        android:id="@+id/imgTypeOther"
        android:layout_width="@dimen/_25sdp"
        android:layout_height="@dimen/_28sdp"
        android:layout_gravity="top"
        android:padding="@dimen/_4sdp"
        android:src="@drawable/ic_other_place"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <TextView
        android:id="@+id/tvTittleOther"
        style="@style/textview_14_medium_black"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:layout_marginLeft="@dimen/_5sdp"
        android:layout_marginRight="@dimen/_5sdp"
        android:layout_weight="1"
        android:textColor="@color/black"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/imgTypeOther"
        tools:text="Name" />

    <TextView
        android:id="@+id/tvAddressFullOther"
        style="@style/textview_12_regular_black"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:layout_marginLeft="@dimen/_5sdp"
        android:layout_marginRight="@dimen/_5sdp"
        android:layout_weight="1"
        android:textColor="@color/colorADABAB"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/imgTypeOther"
        app:layout_constraintTop_toBottomOf="@+id/tvTittleOther"
        tools:text="Name" />
    <!--            tools:text="1st Floor,10-Kalpana Soc B/h.Municipal Market, Mithakhali, Navrangpura, Ahmedabad, Gujarat 380009, India1st Floor,10-Kalpana Soc B/h.Municipal Market, Mithakhali, Navrangpura, Ahmedabad, Gujarat 380009, India1st Floor,10-Kalpana Soc B/h.Municipal Market, Mithakhali, Navrangpura, Ahmedabad, Gujarat 380009, India" />-->


